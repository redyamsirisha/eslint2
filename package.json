  "lint-staged": {
    "src/**/*.{js,ts,json}": [
      "jest --bail --findRelatedTests",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/missing-types.d.ts",
      "!src/react-app-env.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 78,
        "branches": 58,
        "lines": 78,
        "functions": 65
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "cross-env CI=true npm run test -- --coverage --watchAll=false"
    }
  }
}
